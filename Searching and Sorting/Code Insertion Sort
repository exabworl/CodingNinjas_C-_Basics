void insertionSort(int *input, int size)
{
    //Write your code here
    for(int i=1;i<size;i++)
    {
    int x=input[i],k;
    for(int j=i-1;j>=0;j--)
    {
        if(input[j]>x)
        {
           input[j+1]=input[j] ;
           k=j;
            input[k]=x;
        }
        
    }
    }
    
}
