void sortZeroesAndOne(int *input, int size)
{
    //Write your code here
    int ar[1000],j=0;
    for(int i=0;i<size;i++)
    {
        if(input[i]==0)
        {
            input[j++]=input[i];
            //cout<<ar[j]<<" ";
        }
    }
    int i=0,k=j;
    while(i<size-j)
    {
        input[k++]=1;
        //cout<<1<<" ";
        i++;
    }
}
