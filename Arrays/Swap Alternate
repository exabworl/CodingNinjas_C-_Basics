void swapAlternate(int *arr, int size)
{
    //Write your code here
    
    if(size%2==0)
    {
        for(int i=0;i<=size;i++)
    {
        if(i%2==0)
        {
            int a;
            a=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=a;
        }
    }
    }
    else
    {
        for(int i=0;i<size-1;i++)
    {
        if(i%2==0)
        {
            int a;
            a=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=a;
        }
    }
        
    }
}
