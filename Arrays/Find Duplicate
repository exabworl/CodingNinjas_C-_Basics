int duplicateNumber(int *arr, int size)
{
    //Write your code here
     int a,x=1;
    if (size==1)
    {
        return arr[0];
    }
    else
    {
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<size;j++)
        {
            if(arr[i]==arr[j]&&i!=j)
            { 
                a=arr[i];
                break;
                
            }
            if(j==(size-1))
                x++;
        }
       //a =arr[i];
    }
    }
    return a;
}
