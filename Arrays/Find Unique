int findUnique( int *arr , int size )
{
    int a,x=1;
    if (size==1)
    {
        return arr[0];
    }
    else
    {
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<size;j++)
        {
            if(arr[i]==arr[j]&&i!=j)
            { 
                x++;
                break;
                
            }
            else if(arr[i]!=arr[j]&&i!=j)
            {
                a=arr[i];
                //x--;
            }
        }
       //a =arr[i];
    }
    }
    return a;
}
