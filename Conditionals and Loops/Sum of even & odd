#include<iostream>
using namespace std;

int main() {
	// Write your code here
    double n;
    float k;
    
  
  cin>>n;
  int j=n,a,sum1=0,sum2=0;
  
  while(n>=0.1)
  {
      k=(n-j)*10;
      a=k;
     // cout<<n<<endl;
      //cout<<a<<endl;
      if(a%2==0)
      {
          sum1=sum1+a;
          //cout<<sum1<<endl;
      }
      if(a%2==1)
      {
          sum2=sum2+a;
          //cout<<sum2<<endl;
      }
       
      n=n/10;
      j=j/10;
     
  }
  cout<<sum1<<" "<<sum2;
    
    return 0;
	
}
