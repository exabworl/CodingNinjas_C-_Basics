#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
      int n,i=1,j,k,m;
    cin>>n;
    //m=n-1;
    while(i<=n)
    {
        m=n-i;
        while(m>0)
        {
            cout<<" ";
            m--;
        }
        k=i;
        int j=1;
        while(j<=i)
        {
            cout<<k;
            k++;
            j++;
        }
        cout<<endl;
        i++;
        //m--;
    }
    return 0;

}
