#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
    int  x,y,z,cel,s;
    cin>>x>>y>>z;
    s=x;
    while(x>=0&&x<=90&&y>=x&&y<=900&&z>=0&&z<=80&&s<=y)
    {
        cel=5.0/9*(s-32);
        cout<<s<<" "<<cel<<endl;
        s=s+z;
       // z=z+d;
    }
  return 0;
}
