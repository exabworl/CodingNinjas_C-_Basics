#include<iostream>
using namespace std;

int main() {
	// Write your code here
    int x,n,a,j=1;
    cin>>x>>n;
    a=x;
    if(x>=0&&x<=8&&n>=1&&n<=9)
    {
        while(j<n)
        {
            a=a*x;
            j++;
        }
        cout<<a;
    }
    else if(n==0)
    {
        a=1;
        cout<<a;
    }
        
	return 0;
}
