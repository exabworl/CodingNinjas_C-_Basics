#include<iostream>
using namespace std;

int main() {
	// Write your code here
    int n;
    cin>>n;
    double i=1.0,j=1.0,k;
    if(n>0)
    {
        while(i<=n/2)
    {
        j=i*i;
        if(n/j>=1&&n/j<2)
        {
            k=i;
        }
        
        i++;
        
        //if(n/j>1)
        //{
          //  cout<<i;
        //}
    }
        
    }
    else
            k=0;
    cout<<k;
	return 0;
}
