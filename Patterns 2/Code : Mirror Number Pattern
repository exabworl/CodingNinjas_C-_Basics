#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
      int n,i=1,j;
    cin>>n;
    while(i<=n)
    {
        j=i;
        while(n>j)
        {
            cout<<" ";
            j++;
        }
        j=1;
        int k=1;
        while(j<=i)
        {
            cout<<k;
            j++;
            k++;
        }
        cout<<endl;
            i++;
        
    }
    return 0;
  
}


