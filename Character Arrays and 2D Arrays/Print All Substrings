void printSubstrings(char input[]) {
    // Write your code here
    for(int i=0;i<strlen(input);i++)
    {
        for(int j=0,k;j<strlen(input);j++)
        {
            for(k=j;k<=j;k++)
            {
                cout<<input[k];
            }
            if(k==j)
                break;
            cout<<endl;
        }
      //  cout<<endl;
    }
        
}
